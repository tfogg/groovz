
<div class="global_feed">
	<div class="form-group search">
		<h1>Search artists and venues</h1>
		<input type="text" class="form-control" placeholder="Search">

	</div>
	<div class="display">
	  	<%- @users.each do |user| %>
	  		<div class="user">
	  			<div class="user_top">
	  				<%= link_to (image_tag(user.gravatar_url)), profile_path(user) %>
	  			</div>
	  			<div class="user_bottom">
	      			<strong><%= link_to user.full_name %></strong>
	      			<p><%= user.role %></p>
	      		</div>


	      	</div>

	    <% end %>
	</div>

</div>



